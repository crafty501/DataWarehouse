<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.dis2016.model.Shop" table="shop">
		<id name="name" type="string" column="name" />
		<property name="stadt" type="string" column="stadt" />
		<property name="region" type="string" column="region" />
		<property name="land" type="string" column="land" />
	</class>
</hibernate-mapping>