<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.dis2016.model.Artikel" table="artikel">
		<id name="name" type="string" column="name" />
		<property name="preis" type="double" column="preis" />
		<property name="groupname" type="string" column="groupname" />
		<property name="familyname" type="string" column="familyname" />
		<property name="categoryname" type="string" column="categoryname" />
	</class>
</hibernate-mapping>