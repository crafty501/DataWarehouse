<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.dis2016.model.Sales" table="sales">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="artikel" type="string"/>
		<property name="shop" type="string"/>
		<property name="datum" type="date" column="datum" />
		<property name="verkauft" type="integer" column="verkauft" />
		<property name="umsatz" type="double" column="umsatz" />
					
	</class>
</hibernate-mapping>